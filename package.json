{
    "name": "b64folder",
    "displayName": "Base64 Folder",
    "description": "Fold and unfold Base64 blocks in any file.",
    "publisher": "cirnovsky",
    "icon": "logo.webp",
    "repository": {
	    "type": "git",
	    "url": "https://github.com/cirnovsky/b64folder"
    },
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.78.0"
    },
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "b64folder.fold",
                "title": "Fold Base64 Blocks"
            },
            {
                "command": "b64folder.unfold",
                "title": "Unfold Base64 Blocks"
            }
        ]
    }
}
